void processValues(int a, int b, int c) {
    int result;
    static int counter;

    // Boucle
    while (a > 0) {
        a = a - 1;
        result = result + a;
    }

    // Condition
    if (b > c) {
        result = result * 2;
    } else {
        result = result / 2;
    }

    counter = counter + result;
    printResult(counter);
}

