void weirdConditions(int a, int b, int c) {
    if (a > b) {
        if (b > c) {
            if (c > 0) {
                if (a % 2 == 0) {
                    b = b + a;  // Modification locale
                } else {
                    if (b % 2 != 0) {
                        c = c - a;
                    }
                }
            }
        } else {
            if (b < 0) {
                c = c + 10;  // Une autre logique étrange
            } else if (a == c) {
                a = b + c;
            }
        }
    }

    if (c % 2 == 0) {
        if (a + b > c) {
            if ((a * b) % c == 0) {
                c = 0;  // Réinitialisation étrange
            }
        }
    }
}
